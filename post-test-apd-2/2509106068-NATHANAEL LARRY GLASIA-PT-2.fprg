        <attribute name="edited" value="SGV3bGV0dCBQYWNrYXJkO0xBUFRPUC0yUjVIMkNKVTsyMDI1LTA5LTI0OzA5OjU0OjU0IFBNOzI7MzkwNQ=="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="suhu" type="Real" array="True" size="6"/>
            <declare name="Bolean" type="Boolean" array="False" size=""/>
            <declare name="NIM" type="Integer" array="False" size=""/>
            <declare name="rata2, jumlah, suhu1f, suhu2f, suhu3k, suhu4k, suhu5r, suhu6r" type="Real" array="False" size=""/>
            <assign variable="suhu[0]" expression="27"/>
            <assign variable="suhu[1]" expression="33"/>
            <assign variable="suhu[2]" expression="46"/>
            <assign variable="suhu[3]" expression="55"/>
            <assign variable="suhu[4]" expression="67"/>
            <assign variable="suhu[5]" expression="92"/>
            <assign variable="suhu1f" expression="(9/5*suhu[0]) + 32"/>
            <assign variable="suhu2f" expression="(9/5*suhu[1]) + 32"/>
            <assign variable="suhu3k" expression="suhu[2] + 273.15"/>
            <assign variable="suhu4k" expression="suhu[3] + 273.15"/>
            <assign variable="suhu5r" expression="(4/5*suhu[4])"/>
            <assign variable="suhu6r" expression="(4/5*suhu[5])"/>
            <assign variable="jumlah" expression="suhu1f + suhu2f + suhu3k + suhu4k + suhu5r + suhu6r"/>
            <assign variable="rata2" expression="jumlah/6"/>
            <assign variable="NIM" expression="68"/>
            <assign variable="Bolean" expression="NIM &lt; rata2"/>
            <output expression="&quot;Suhu : &quot; &amp; suhu[0] &amp; &quot;, &quot; &amp; suhu[1] &amp; &quot;, &quot; &amp; suhu[2] &amp; &quot;, &quot; &amp; suhu[3] &amp; &quot;, &quot; &amp; suhu[4] &amp; &quot;, &quot; &amp; suhu[5]" newline="True"/>
            <output expression="&quot;suhu[0] = &quot; &amp; suhu1f" newline="True"/>
            <output expression="&quot;suhu[1] = &quot; &amp; suhu2f" newline="True"/>
            <output expression="&quot;suhu[2] = &quot; &amp; suhu3k" newline="True"/>
            <output expression="&quot;suhu[3] = &quot; &amp; suhu4k" newline="True"/>
            <output expression="&quot;suhu[4] = &quot; &amp; suhu5r" newline="True"/>
            <output expression="&quot;suhu[5] = &quot; &amp; suhu6r" newline="True"/>
            <output expression="&quot;Jumlah = &quot; &amp; jumlah" newline="True"/>
            <output expression="&quot;Rata-rata = &quot; &amp; rata2" newline="True"/>
            <output expression="&quot;NIM = &quot; &amp; NIM" newline="True"/>
            <output expression="&quot;Bolean (NIM &lt; rata2) = &quot; &amp; Bolean" newline="True"/>
        </body>
    </function>
</flowgorithm>
